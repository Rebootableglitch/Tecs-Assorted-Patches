	</mods>
		<match Class="PatchOperationSequence">
			<operations>

			<li Class="PatchOperationAdd">
				<xpath>Defs</xpath>
				<value>
					<ArchitectSense.DesignationSubCategoryDef>
						<defName>ShieldGenerators_FD</defName>
						<label>Shield Generators</label>
						<description>Shield generators that can be depoyed for combat.</description>
						<defNames>
							<li>Building_ShieldGenerator</li>
							<li>Building_ShieldGeneratorLarge</li>
							<li>Building_ShieldGeneratorPortable</li>
						</defNames>
						<designationCategory>Security</designationCategory>
						<debug>false</debug>
					</ArchitectSense.DesignationSubCategoryDef>
				</value>
			</li>

			</operations>
		</match>
    </Operation>
	
</Patch>